name: Iris-Classifier-Tutorial

python_env: python_env.yaml

entry_points:
  main:
    parameters:
      C: {type: float, default: 1.0}
      max_iter: {type: int, default: 200}
      experiment_name: {type: string, default: None}
      register_model_name: {type: string, default: "None"}
    command: "python train.py --C {C} --max_iter {max_iter} --experiment-name {experiment_name} --register-model-name {register_model_name}"